                            <li><a href="/Admin/Category/List"><i class="fa fa-circle-o"></i> Kategori Listele</a></li>
                         
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-laptop"></i>
                            <span>Haberler</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/Admin/Post/Create"><i class="fa fa-circle-o"></i> Haber Ekle</a></li>
                            <li><a href="/Admin/Post/List"><i class="fa fa-circle-o"></i> Haber Görüntüle</a></li>
                           
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-user"></i> <span>Kullanıcılar</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/Admin/AppUser/Create"><i class="fa fa-circle-o"></i> Kullanıcı Ekle</a></li>
                            <li><a href="/Admin/AppUser/List"><i class="fa fa-circle-o"></i> Kullanıcı Görüntüle</a></li>
                            
                        </ul>
                    </li>
                  
                    

                  
                  
                  
                </ul>
            </section>
